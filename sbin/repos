DLA_REPOS=()
for repo in $DLA_REPOS_ORIGIN_REMOTE; do
  name=$(cut -d'|' -f1 <<<$repo)
  origin=$(cut -d'|' -f2 <<<$repo)
  remote=$(cut -d'|' -f3 <<<$repo)
  branch=$(cut -d'|' -f4 <<<$repo)
  DLA_REPOS+=($name)
  DLA_REPOS+=($origin)
  DLA_REPOS+=($remote)
  DLA_REPOS+=($branch)
done
