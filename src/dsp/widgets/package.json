    "webpack-dev-server": "3.11.0",
    "html-webpack-plugin": "4.3.0",
    "util": "0.12.2",
    "crypto-browserify": "3.12.0",
    "buffer": "5.5.0",
    "events": "3.1.0",
    "stream-browserify": "2.0.2",
    "vm-browserify": "1.1.2",
    "path-browserify": "1.0.1",
    "process": "0.11.10"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/setupTests.{js,jsx}",
      "!src/index.{js,jsx}",
      "!src/serviceWorker.{js,jsx}",
      "!src/**/*.(spec|test|stories).{js,jsx}"
    ]
  }
