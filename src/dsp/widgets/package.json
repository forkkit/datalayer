    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/chartist": "0.9.48",
    "@types/node": "13.11.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "5.0.1",
    "@types/webpack-env": "1.15.1",
    "@types/jest": "25.2.1",
    "babel-loader": "^8.0.6",
    "gulp": "4.0.2",
    "gulp-filter": "6.0.0",
    "gulp-watch": "5.0.1",
    "gulp-append-prepend": "1.0.8",
    "serve": "^11.3.0",
    "npm-run-all": "4.1.5",
    "style-loader": "1.1.3",
    "css-loader": "3.5.0",
    "url-loader": "4.0.0",
    "svg-url-loader": "5.0.0",
