    "@jupyterlab/application": "2.1.0",
    "@jupyterlab/apputils": "2.1.0",
    "@jupyterlab/codemirror": "2.1.0",
    "@jupyterlab/theme-light-extension": "2.1.0",
    "@jupyterlab/ui-components": "2.1.0",
    "animate.css": "3.7.2",
